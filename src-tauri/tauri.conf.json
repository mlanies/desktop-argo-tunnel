{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Desktop argo tunnel",
  "mainBinaryName": "Desktop argo tunnel",
  "version": "0.3.0",
  "identifier": "com.desktop-argo-tunnel.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "shadow": false,
        "center": true,
        "decorations": false,
        "transparent": true,
        "fullscreen": false,
        "resizable": false,
        "title": "Desktop argo tunnel",
        "width": 900,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": ["./binaries/cloudflared"]
  }
}
